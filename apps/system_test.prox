// System Test Suite

func assert(cond, msg) {
    if (cond) {
        print("PASS: " + msg);
    } else {
        print("FAIL: " + msg);
    }
}

// 1. Math
assert(1 + 1 == 2, "Addition");
assert(10 * 2 == 20, "Multiplication");

// 2. Logic
assert(true and true, "Boolean AND");
assert(not false, "Boolean NOT");

// 3. Variables
let x = 100;
assert(x == 100, "Variable Access");
