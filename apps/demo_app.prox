// Demo App: Calculator with History

class Calculator {
    func add(a, b) {
        return a + b;
    }
    
    func sub(a, b) {
        return a - b;
    }
}

let calc = Calculator();
let res = calc.add(10, 5);

if (res > 10) {
    print("Result is large: " + res);
} else {
    print("Result is small");
}

print("Running loop...");
for (let i = 0; i < 3; i = i + 1) {
    print(i);
}
