// benchmarks/string_concat.prox
// Stress Test: Allocation & GC

const ITERATIONS = 20000;
print("Starting String Concat...");

let start = clock();
let s = "";
let i = 0;
while (i < ITERATIONS) {
    s = s + ".";
    i = i + 1;
}
let end = clock();

print("String Length: " + len(s));
print("Time: " + (end - start));
