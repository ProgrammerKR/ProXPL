// Debug Shadowing
print("Starting Shadowing Debug");
let x = 10;
print("Outer x: ");
print(x);

if (true) {
    print("Entering block...");
    let x = 20;
    print("Inner x: ");
    print(x);
    if (x == 20) {
        print("Inner Check: 20 (Correct)");
    } else {
        print("Inner Check: FAIL");
    }
}

print("Outer x after block: ");
print(x);
if (x == 10) {
    print("Outer Check: 10 (Correct)");
} else {
    print("Outer Check: FAIL");
}
